{
  "compilerOptions": {
    "traceResolution": false,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "es2015",
      "es2016",
      "es6",
      "es5",
      "dom"
    ],
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "allowJs": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noStrictGenericChecks": true
  },
  "exclude": [
    "node_modules"
  ]
}
